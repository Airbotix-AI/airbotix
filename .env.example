# Airbotix Website Environment Variables
# Copy this file to .env.local and update with your actual values

# API Configuration
VITE_API_BASE_URL=https://api.airbotix.com
VITE_API_VERSION=v1

# Contact Form
VITE_CONTACT_EMAIL=info@airbotix.com
VITE_CONTACT_PHONE=+61123456789

# Analytics (Optional)
VITE_GA_TRACKING_ID=
VITE_GTM_ID=

# Social Media
VITE_LINKEDIN_URL=https://www.linkedin.com/company/airbotix-ai
VITE_FACEBOOK_URL=
VITE_INSTAGRAM_URL=
VITE_YOUTUBE_URL=

# Feature Flags
VITE_ENABLE_BOOKING=false
VITE_ENABLE_PAYMENTS=false
VITE_ENABLE_CHAT=false

# Development
VITE_APP_ENV=development
VITE_DEBUG_MODE=true