{
  "name": "airbotix-website",
  "version": "1.0.0",
  "description": "Static website for Airbotix - AI and Robotics education for K-12 students",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
